class Fan:
    def __init__(self,nom,ustoz):
        self.nom=nom
        self.ustoz=ustoz
    def chiqarish(self):
        return (f"fan:{self.nom}-ustoz:{self.ustoz}")
algebra=Fan("Algebra","A.Aliyev")
geografiya=Fan("Geografiya","X.Shamsiyeva")
print(algebra.chiqarish())
print(geografiya.chiqarish())
